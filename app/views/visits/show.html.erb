<h1>Information for Grocery Store Visit on <%=@visit.date.strftime("%d of %B, %Y")%> </h1>
<h4> Store Location: </h4>
<%@visit.grocery_store.chain%> <%=link_to @visit.grocery_store.address, grocery_store_path(@visit.grocery_store) %>
<h4> User: </h4>
<%=@visit.user.name%>
<h4> Date: <%=@visit.date.strftime("%d of %B, %Y")%> </h4>
<h4> Notes: </h4>
<p><%=@visit.notes%></p>
<%= link_to "Edit", edit_visit_path%>
<%= link_to "Delete", @visit, method: "delete", data: {confirm: "Are you sure you want to delete this visit"}%>